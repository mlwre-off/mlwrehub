    G2L["14"]["CornerRadius"] = UDim.new(0.5, 0); -- Более округлые углы для современного дизайна

    -- StarterGui.DeltaGui.KeySystem.MainKeyFrame.Continue.ImageLabel
    G2L["15"] = Instance.new("ImageLabel", G2L["f"]);
    G2L["15"]["BackgroundColor3"] = Color3.fromRGB(255, 0, 0);
    G2L["15"]["Image"] = [[rbxassetid://125787475899820]];
    G2L["15"]["Size"] = UDim2.new(0.1541670262813568, 0, 0.5313842296600342, 0);
    G2L["15"]["BackgroundTransparency"] = 1;
    G2L["15"]["Position"] = UDim2.new(0.6577792763710022, 0, 0.2479792982339859, 0);

    -- Остальная часть кода для UI...
    -- [Здесь должен быть остальной код, который не был изменен]

    -- Применяем такие же улучшения дизайна ко всем остальным кнопкам и элементам
    -- аналогично тому, как было сделано для MainKeyFrame и Continue
    
    -- Например, добавляем аналогичные улучшения для кнопки GetKey:
    G2L["16"]["BackgroundColor3"] = Color3.fromRGB(235, 30, 70); -- Тот же красный
    -- Добавляем градиент
    local getKeyGradient = Instance.new("UIGradient", G2L["16"])
    getKeyGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 50, 90)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(220, 20, 60))
    })
    getKeyGradient.Rotation = 90
    
    -- Добавляем тень
    local getKeyShadow = Instance.new("ImageLabel", G2L["16"])
    getKeyShadow.Name = "Shadow"
    getKeyShadow.BackgroundTransparency = 1
    getKeyShadow.Image = "rbxassetid://5554236805"
    getKeyShadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
    getKeyShadow.ImageTransparency = 0.7
    getKeyShadow.Position = UDim2.new(0, -5, 0, -5)
    getKeyShadow.Size = UDim2.new(1, 10, 1, 10)
    getKeyShadow.ScaleType = Enum.ScaleType.Slice
    getKeyShadow.SliceCenter = Rect.new(23, 23, 277, 277)
    getKeyShadow.ZIndex = G2L["16"].ZIndex - 1
    
    -- Аналогичные эффекты наведения и нажатия для GetKey
    G2L["17"].MouseEnter:Connect(function()
        game:GetService("TweenService"):Create(
            G2L["16"],
            TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out),
            {BackgroundColor3 = Color3.fromRGB(255, 70, 100)}
        ):Play()
    end)
    
    G2L["17"].MouseLeave:Connect(function()
        game:GetService("TweenService"):Create(
            G2L["16"],
            TweenInfo.new(0.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out),
            {BackgroundColor3 = Color3.fromRGB(235, 30, 70)}
        ):Play()
    end)
    
    G2L["17"].MouseButton1Down:Connect(function()
        game:GetService("TweenService"):Create(
            G2L["16"],
            TweenInfo.new(0.1, Enum.EasingStyle.Quint, Enum.EasingDirection.Out),
            {Size = UDim2.new(G2L["16"].Size.X.Scale * 0.95, 0, G2L["16"].Size.Y.Scale * 0.95, 0)}
        ):Play()
    end)
    
    G2L["17"].MouseButton1Up:Connect(function()
        game:GetService("TweenService"):Create(
            G2L["16"],
            TweenInfo.new(0.2, Enum.EasingStyle.Elastic, Enum.EasingDirection.Out),
            {Size = UDim2.new(G2L["16"].Size.X.Scale / 0.95, 0, G2L["16"].Size.Y.Scale / 0.95, 0)}
        ):Play()
    end)
    
    -- Также улучшаем дизайн JoinServer кнопки
    G2L["1d"]["BackgroundColor3"] = Color3.fromRGB(70, 120, 220); -- Синий для разнообразия
    -- Добавляем градиент
    local joinServerGradient = Instance.new("UIGradient", G2L["1d"])
    joinServerGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(90, 140, 240)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(70, 120, 220))
    })
    joinServerGradient.Rotation = 90
    
    -- Тут и далее следует применить аналогичные улучшения для остальных элементов UI
    -- включая MainUi, боковую панель, вкладки и т.д.
    
    -- Применяем современные анимации для переключения вкладок
    local animations = createUIAnimations()
    local tabSwitcher = animations.tabSwitcher(G2L["46"], 0.4)
    
    -- Функция, которая запускается при запуске скрипта и применяет
    -- анимации и эффекты ко всем подходящим элементам интерфейса
    local function applyUIImprovements()
        -- Применяем тени ко всем кнопкам
        for _, obj in pairs(G2L) do
            if obj:IsA("Frame") and obj.Name:find("Button") then
                animations.applyShadow(obj)
            end
            
            -- Применяем эффекты наведения и нажатия ко всем кнопкам
            if obj:IsA("TextButton") or obj:IsA("ImageButton") then
                animations.hover(obj)
                animations.buttonPress(obj)
            end
            
            -- Применяем современные скругления ко всем UI элементам
            if (obj:IsA("Frame") or obj:IsA("TextButton") or obj:IsA("TextBox")) 
                and not obj:FindFirstChildOfClass("UICorner") then
                animations.applyRounding(obj, 0.12)
            end
        end
        
        -- Применяем плавное появление для главных элементов UI при запуске
        if G2L["25"] then -- MainFrame
            G2L["25"].BackgroundTransparency = 1
            animations.appear(G2L["25"], 0.8)
        end
    end
    
    -- Запускаем улучшения UI
    spawn(applyUIImprovements)
    
    -- Остальной код остается без изменений...
    -- [Здесь должен быть оставшийся код]
